{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "types": ["cypress", "@badeball/cypress-cucumber-preprocessor"],
      "moduleResolution": "node",
      "baseUrl": "https://www.google.co.uk",
      "outDir": "dist"
    },
    "include": [
      "cypress/**/*.ts"
    ]
  }
  